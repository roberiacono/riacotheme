{
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#f5f5f5",
					"name": "Base / Two",
					"slug": "base-2"
				},
				{
					"color": "#334155",
					"name": "Base / Three",
					"slug": "base-3"
				},
				{
					"color": "#444444",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#ffffff",
					"name": "Contrast / Two",
					"slug": "contrast-2"
				},
				{
					"color": "#00aeef",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#045cb4",
					"name": "Secondary",
					"slug": "secondary"
				}
			]
		},
		"layout": {
			"contentSize": "620px",
			"wideSize": "1280px"
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "\"Open Sans\"",
							"fontStyle": "normal",
							"fontWeight": "300 800",
							"src": [
								"file:./assets/fonts/open-sans/open-sans-300-800-normal.ttf"
							]
						}
					],
					"fontFamily": "\"Open Sans\"",
					"name": "Open Sans",
					"slug": "open-sans"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "8px"
				},
				"variations": {
					"outline": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						}
					}
				}
			},
			"core/post-author": {
				"css": "align-items: center;"
			},
			"core/pullquote": {
				"border": {
					"bottom": {
						"color": "var(--wp--preset--color--primary)",
						"style": "solid"
					},
					"color": null,
					"style": null,
					"top": {
						"color": "var(--wp--preset--color--primary)",
						"style": "solid"
					},
					"width": null
				}
			},
			"core/quote": {
				"border": {
					"color": null,
					"left": {
						"color": "var(--wp--preset--color--primary)",
						"style": "solid"
					},
					"style": null,
					"width": null
				}
			},
			"core/search": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.5"
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--primary)"
				},
				"spacing": {
					"margin": {
						"bottom": "1rem",
						"top": "1rem"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"css": ":root :where(.wp-site-blocks) > * {\n    margin-block-start: 0;\n    margin-block-end: 0;\n}\n",
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--primary)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--open-sans)",
					"fontStyle": "normal",
					"fontWeight": "400",
					"lineHeight": "1.5"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--base-3)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--accent)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "0",
				"right": "0",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--open-sans)",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}